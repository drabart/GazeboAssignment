<?xml version="1.0" ?>
<sdf version="1.9">
  <world name="default">
    <!-- Include the ground plane -->
    <include>
      <uri>file://model/ground_plane.sdf</uri>
      <name>ground_plane</name>
    </include>

    <!-- Include the robot -->
    <include>
      <uri>file://model/robot.sdf</uri>
      <name>robot</name>
      <pose>0 0 1 0 0 0</pose> <!-- slightly above ground -->
    </include>
  </world>
</sdf>
